<h1>Home#index</h1>

<div id="main">
  <div class="left">
    <p>izquierda</p>
  </div>
  <div class="right">
    <p>derecha</p>
  </div>
</div>

<p class="clear"> --- angular empieza aca ---</p>

<div ng-app="ProtoFront" ng-controller="HomeCtrl">

  <div class="eva-message-info ng-scope">
    <div class="message-icon">
      <span class="eva-icon-info"></span>
    </div>
    <div class="message-close">
      <i class="eva-icon-close"></i><!--[if IE 8]>
    <span>&#215;</span>
    <![endif]-->
    </div>
    <div class="message-text">
      <p>Datos de su reserva</p>
    </div>
  </div>


  <div class="eva-grid">
    <div class="eva-col-small-12-medium-4-large-2-huge-2">{{reservation.id}}</div>
    <div class="eva-col-small-12-medium-4-large-2-huge-2">{{reservation.country}}</div>
    <div class="eva-col-small-12-medium-4-large-2-huge-2">{{reservation.mail}}</div>
    <div class="eva-col-small-12-medium-12-large-6-huge-6">
      Vuelo {{reservation.products[0].from}} - {{reservation.products[0].to}} / Hotel {{reservation.products[1].name}}
    </div>
  </div>

</div>

<p class=""> --- aca terminar angular ---</p>

